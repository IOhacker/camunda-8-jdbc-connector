  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:Task"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "id": "jdbc",
      "label": "JDBC"
    },
    {
      "id": "output",
      "label": "Output Mapping"
    },
    {
      "id": "errors",
      "label": "Error Handling"
    }
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "io.camunda:connector-jdbc:1",
      "binding": {
        "type": "zeebe:taskDefinition:type"
      }
    },
    {
      "label": "Driver",
      "description": "Type of database. For example: mysql",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.driverName"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Driver sub protocol",
      "description": "Driver sub protocol. For example: thin",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.driverSubProtocol"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Host",
      "description": "Database Server Host",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.host"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Port",
      "description": "Database Server Port",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.port"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Database Name",
      "description": "Which database to connect to?",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.dbName"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Username",
      "description": "Username to use to connect to the database",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.username"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Password",
      "description": "Password to use to connect to the database",
      "group": "jdbc",
      "type": "String",
      "feel": "optional",
      "binding": {
        "type": "zeebe:input",
        "name": "jdbc.password"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Result Variable",
      "description": "Name of variable to store the response in",
      "group": "output",
      "type": "String",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultVariable"
      }
    },
    {
      "label": "Result Expression",
      "description": "Expression to map the response into process variables",
      "group": "output",
      "type": "Text",
      "feel": "required",
      "binding": {
        "type": "zeebe:taskHeader",
        "key": "resultExpression"
      }
    },
    {
      "label": "Error Expression",
